<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="Add English Strings for Cancel Popover" author="Kevin">
        <loadData encoding="UTF-8"
                  file="changelog/sprint_5/data/em-9.csv"
                  separator=";"
                  tableName="strings"/>
    </changeSet>

    <changeSet id="update_team_edit_page.cancel_dialog.content_value" author="Kevin">
        <update tableName="strings">
            <column name="message" value="Are you sure, any unsaved changes will not be saved"></column>
            <where>key_path = 'team_edit_page.cancel_dialog.content'</where>
        </update>
    </changeSet>

</databaseChangeLog>