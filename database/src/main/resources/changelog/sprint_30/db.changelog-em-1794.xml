<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


    <changeSet id="add ak instead of ix" author="Matt">
        <dropIndex tableName="client_team_email_configuration" indexName="ix_client_team_email_configuration_team"/>
        <createIndex tableName="client_team_email_configuration"
                     indexName="ak_client_team_email_configuration_team"
                     unique="true">
            <column name="client_team_id"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>
